        "mapping": {
          "type": "keyword"
        }
      }
    },
    {
      "s_template": {
        "match": "*_s",
        "mapping": {
          "type": "keyword"
        }
      }
    },
    {
      "conservation_template": {
        "path_match": "data.conservation_*",
        "mapping": {
          "type": "keyword",
          "copy_to": [
            "all"
          ]
        }
      }
    },
    {
      "distribution_template": {
